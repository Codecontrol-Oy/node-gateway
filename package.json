{
  "name": "@codecontrol/node-gateway",
  "version": "1.3.8",
  "description": "A Simple NodeJS based proxy gateway",
  "keywords": [
    "gateway",
    "proxy",
    "api gateway",
    "docker",
    "endpoint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Codecontrol-Oy/node-gateway"
  },
  "main": "index.js",
  "scripts": {
    "test": "npm run clean:report && jest",
    "test:watch": "npm run clean:report && jest --watch",
    "test:ci": "npm run clean:report && jest --coverage",
    "clean": "rimraf ./build",
    "clean:report": "rimraf ./report"
  },
  "jest": {
    "coverageDirectory": "report",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/containers/",
      "/docs/",
      "/build/",
      "/mocks/"
    ],
    "testRegex": "\\.test\\.(js|jsx)$"
  },
  "author": "Toni Iltanen",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "jasmine": "^4.0.2",
    "jest": "^27.4.7",
    "nock": "^13.0.4",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.3",
    "socket.io-client": "^4.2.0",
    "ws": "^8.2.2"
  },
  "dependencies": {
    "http": "0.0.1-security",
    "http-proxy": "^1.18.1",
    "http-proxy-rules": "^1.1.3",
    "jsonschema": "^1.4.0",
    "log4js": "^6.3.0"
  }
}
