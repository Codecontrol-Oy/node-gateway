{
  "name": "@codecontrol/node-gateway",
  "version": "1.2.9",
  "description": "A Simple NodeJS based proxy gateway",
  "keywords": ["gateway", "proxy", "api gateway", "docker", "endpoint"],
  "repository": {
    "type": "git",
    "url": "https://github.com/Codecontrol-Oy/node-gateway"
  },
  "main": "index.js",
  "scripts": {
    "test": "npm run clean:report && jest",
    "test:watch": "npm run clean:report && jest --watch",
    "test:ci": "npm run clean:report && jest --coverage",
    "clean": "rimraf ./build",
    "clean:report": "rimraf ./report"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jasmine.js"
    ],
    "coverageDirectory": "report",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/containers/",
      "/docs/",
      "/build/",
      "/mocks/"
    ],
    "testRegex": "\\.test\\.(js|jsx)$"
  },
  "author": "Toni Iltanen",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "jasmine-reporters": "^2.3.2",
    "jest": "^26.4.2",
    "nock": "^13.0.4",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.3"
  },
  "dependencies": {
    "http": "0.0.1-security",
    "http-proxy": "^1.18.1",
    "http-proxy-rules": "^1.1.3",
    "jsonschema": "^1.2.6",
    "log4js": "^6.3.0",
    "node-http-proxy": "^0.2.3"
  }
}
