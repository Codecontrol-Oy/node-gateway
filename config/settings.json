{
    "settings": {
        "cors": {
            "allowedOrigin": "*",
            "allowCredentials": true,
            "allowedMethods": "GET, POST, DELETE, UPDATE, OPTIONS",
            "allowedHeaders": "*"
        },
        "server": {
            "port": 6010,
            "noRouteMatchesErrorMessage": "No route matches the given address",
            "generalErrorMessage": "node-gateway general error",
            "host": "127.0.0.1",
            "serviceName": "node-gateway"
        },
        "logger": {
            "logconfig": {
                "appenders": {
                    "node-gateway": {
                        "type": "console"
                    }
                },
                "categories": {
                    "default": {
                        "appenders": [
                            "node-gateway"
                        ],
                        "level": "info"
                    }
                }
            },
            "loglevel": "DEBUG"
        }
    }
}